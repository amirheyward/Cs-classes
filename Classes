
Console.WriteLine("Hello, World");
Book book1 = new Book("Hippos", "The World");
book1.Print();
public class Book
{
    public string Title;
    public string Author;
    public Book(string title, string author)
    {
        Title = title;
        Author = author;
        Console.WriteLine("Making a new book");
    }
    
    public void Print()
    {
        Console.WriteLine(Title);
        Console.WriteLine(Author);
    }
}
